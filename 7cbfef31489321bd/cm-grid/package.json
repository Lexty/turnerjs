{
  "name": "cm-grid",
  "version": "1.0.428",
  "description": "Content Management grid",
  "main": "src/index.js",
  "config": {
    "itmochaopts": "--preserve-symlinks --require ./node_modules/dbsm-common-test/src/it/babelRegister.js --require ./node_modules/dbsm-common-test/src/it/setupJSDom.js"
  },
  "scripts": {
    "build": "NODE_ENV=production webpack --progress --config webpack-prod.config.js",
    "test": "npm run test:all && npm run lint",
    "release": "wnpm-release --no-shrinkwrap",
    "start": "webpack-dev-server --progress --config webpack-dev.config.js",
    "lint": "eslint --cache --ext .js,.jsx .",
    "test:quick": "mocha 'test/unit/**/*.test.js' 'test/it/**/*.test.js' $npm_package_config_itmochaopts",
    "test:browser": "mocha 'test/browser/**/*.test.js'",
    "test:all": "mocha 'test/unit/**/*.test.js' 'test/it/**/*.test.js' 'test/browser/**/*.test.js' $npm_package_config_itmochaopts --reporter mocha-env-reporter",
    "watch": "npm run test:quick -- --watch --watch-extensions jsx",
    "prepush": "npm prune && npm install && npm run build && npm test",
    "test:unit": "mocha 'test/unit/**/*.test.js' $npm_package_config_itmochaopts",
    "test:it": "mocha 'test/it/**/*.test.js' $npm_package_config_itmochaopts"
  },
  "publishConfig": {
    "registry": "http://repo.dev.wix/artifactory/api/npm/npm-local/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/wix-private/wix-code-cm-ui.git"
  },
  "keywords": [
    "editor",
    "cm",
    "react",
    "redux"
  ],
  "author": "David Zuckerman<davidz@wix.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/wix-private/dbsm/issues"
  },
  "homepage": "https://github.com/wix-private/wix-code-cm-ui",
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-react-jsx": "^6.8.0",
    "babel-plugin-transform-react-jsx-self": "^6.11.0",
    "babel-plugin-transform-react-jsx-source": "^6.9.0",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.9.0",
    "case-sensitive-paths-webpack-plugin": "^1.1.0",
    "chai": "^3.5.0",
    "cm-common-test": "*",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.1",
    "dbsm-common": "*",
    "dbsm-common-test": "*",
    "eslint": "^2.11.1",
    "eslint-config-standard": "^5.3.1",
    "eslint-config-standard-jsx": "^1.2.1",
    "eslint-config-standard-react": "^2.4.0",
    "eslint-plugin-mocha": "^3.0.0",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^5.1.1",
    "eslint-plugin-standard": "^1.3.2",
    "express": "^4.14.0",
    "ignore-styles": "^4.0.0",
    "mocha": "^2.5.3",
    "mocha-env-reporter": "^1.0.2",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "raven-js": "^3.7.0",
    "sass-loader": "^4.0.0",
    "selenium-webdriver": "^2.53.2",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "wix-data-testkit": "^1.0.18",
    "wnpm-ci": "*"
  },
  "dependencies": {
    "ag-grid": "6.4.2",
    "ag-grid-enterprise": "6.4.2",
    "ag-grid-react": "6.4.0",
    "classnames": "^2.2.5",
    "co": "^4.6.0",
    "lodash": "^4.13.1",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "sinon": "^1.17.5",
    "sinon-chai": "^2.8.0",
    "wix-base-ui": "^1.1121.0",
    "wix-code-media-manager-support": "*"
  }
}
