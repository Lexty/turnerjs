{
    "parserOptions": {
        "ecmaVersion": 6
    },
    "plugins": [
        "jasmine"
    ],
    "rules": {
        "quote-props": [0, "as-needed"],
        "no-multi-spaces": 2,
        "key-spacing": [2, { "beforeColon": false, "afterColon": true }],
        "comma-spacing": 2,
        "quotes": [0, "single", "avoid-escape"],
        "no-extra-parens": 0,
        "indent": [0, 4, {"SwitchCase": 1}],
        "func-style": [2, "declaration"],
        "no-use-before-define": 0,
        "consistent-this": 2,
        "space-before-function-paren": [0, {"anonymous": "always", "named": "never"}],
        "space-before-blocks": [0, "always"],
        "operator-linebreak": [2, "after"],
        "dot-location": [2, "property"],
        "semi-spacing": [2, {"before": false, "after": true}],
        "array-bracket-spacing": [2, "never"],
        "object-curly-spacing": [0, "never"],
        "no-throw-literal": 2,
        "jasmine/no-spec-dupes": 0,
        "jasmine/no-suite-dupes": 0,
        "jasmine/missing-expect": 0,
        "jasmine/no-suite-callback-args": 2,
        "jasmine/no-focused-tests": 2,
        "jasmine/valid-expect": 2,
        "jasmine/no-disabled-tests": 0
    },
    "env": {
        "jasmine": true,
        "browser": true,
        "node": true
    },
    "globals": {
        "Promise": true
    }
}
